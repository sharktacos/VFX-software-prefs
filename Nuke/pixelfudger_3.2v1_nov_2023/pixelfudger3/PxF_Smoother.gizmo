  inputs 2
  operation divide
  name Merge2
  xpos 45
  ypos -26
 }
push $N201d400
 Blur {
  size {{parent.bigBlur.w} {parent.bigBlur.h}}
  name Blur2
  xpos 228
  ypos -127
 }
 Merge2 {
  inputs 2
  operation multiply
  name Merge3
  selected true
  xpos 228
  ypos -6
 }
 Gamma {
  value {{"1 / parent.gamma"}}
  name Gamma2
  xpos 228
  ypos 66
 }
 ShuffleCopy {
  inputs 2
