  value {{parent.gamma}}
  name Gamma1
  xpos 169
  ypos -218
 }
set N201d400 [stack 0]
 Dot {
  name Dot2
  xpos 26
  ypos -107
 }
push $N201d400
 Blur {
  size {{parent.smallBlur.w} {parent.smallBlur.h}}
  name Blur1
  xpos 115
  ypos -125
 }
